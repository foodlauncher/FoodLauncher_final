<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.launcher.foodlauncher.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_restaurants"
        android:name="com.launcher.foodlauncher.ui.home.RestaurantsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_restaurants" />

    <fragment
        android:id="@+id/navigation_find_food"
        android:name="com.launcher.foodlauncher.ui.find_food.FindFoodFragment"
        android:label="@string/title_find_food"
        tools:layout="@layout/fragment_find_food" />

    <fragment
        android:id="@+id/navigation_favourites"
        android:name="com.launcher.foodlauncher.ui.favourites.FavouritesFragment"
        android:label="@string/title_favourites"
        tools:layout="@layout/fragment_favourites" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.launcher.foodlauncher.ui.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user" />

</navigation>
